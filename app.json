{
  "expo": {
    "name": "PaysysShopApp",
    "slug": "PaysysShopApp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#52F9D1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#52F9D1"
      },
      "package": "com.paysysshopappv",
      "permissions": []
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
